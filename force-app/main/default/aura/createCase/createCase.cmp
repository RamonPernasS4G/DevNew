<aura:component implements="lightning:availableForFlowScreens" access="global" controller="CreateCase">
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>

    <aura:attribute access="global" name="companyId" type="String" default=""></aura:attribute>
    <aura:attribute access="global" name="clientId" type="String" default=""></aura:attribute>
    <aura:attribute access="global" name="asunto" type="String" default=""></aura:attribute>
    <aura:attribute access="global" name="descripcion" type="String" default=""></aura:attribute>
    <aura:attribute access="global" name="motivo" type="String" default=""></aura:attribute>
    <aura:attribute access="global" name="respuesta" type="String" default=""></aura:attribute>

    <aura:attribute access="global" name="name" type="String" default=""></aura:attribute>
    <aura:attribute access="global" name="address" type="String" default=""></aura:attribute>
    <aura:attribute access="global" name="relacion" type="String" default=""></aura:attribute>

    <aura:attribute name="url" type="String"/>
    <aura:attribute name="pageReference" type="Object"/>
    <lightning:navigation aura:id="navService"/>
</aura:component>